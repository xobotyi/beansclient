{
  "name": "xobotyi/beansclient",
  "description": "PHP7.1+ client for beanstalkd work queue with no dependencies",
  "type": "library",
  "keywords": [
    "php",
    "beanstalk",
    "beanstalkd",
    "queue",
    "client"
  ],
  "license": "MIT",
  "homepage": "https://github.com/xobotyi/beansclient",
  "support": {
    "issues": "https://github.com/xobotyi/beansclient/issues"
  },
  "authors": [
    {
      "name": "Anton Zinovyev",
      "email": "xog3@yandex.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "suggest": {
    "ext-sockets": "Provides most robust interconnection with sockets"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0.0",
    "codacy/coverage": "^1.0.0",
    "ext-sockets": "*"
  },
  "autoload": {
    "psr-4": {
      "xobotyi\\beansclient\\": "src"
    }
  }
}
